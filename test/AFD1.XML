<!-- palavras numeros inteiros sem 0 nao significativos -->

<AFD>
    <simbolos>
        <bloco valor="0-9"/>
    </simbolos>

    <delimitador valor=";"/>

    <estados>
        <elemento valor="A"/>
        <elemento valor="B"/>
        <elemento valor="C"/>
    </estados>

    <estadosFinais>
        <elemento valor="B"/>
        <elemento valor="C"/>
    </estadosFinais>

    <funcaoPrograma>

        <elemento origem="A">
            <destino destino="B" simbolo="0"/>
            <destino destino="C" simbolo="1-9"/>
        </elemento>

        <elemento origem="C">
            <destino destino="C" simbolo="0-9"/>
        </elemento>

    </funcaoPrograma>

    <estadoInicial valor="A"/>
</AFD>
